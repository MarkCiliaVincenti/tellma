@page "/identity/forgot-password-confirmation"
@inject IHtmlLocalizer<Strings> Localizer
@model ForgotPasswordConfirmation
@{
    ViewData["Title"] = Localizer["ForgotPasswordConfirmation"];
}

<div class="text-center pt-3">
    <div class="alert-success p-2">
        <i class="fas fa-check"></i>&nbsp;
        <span>
            @Localizer["ForgotPasswordConfirmationMessage"]
        </span>
    </div>
</div>



