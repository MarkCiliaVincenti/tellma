{
  "index": "/index.html",
  "assetGroups": [
    {
      "name": "app",
      "installMode": "prefetch",
      "resources": {
        "files": [
          "/favicon.ico",
          "/index.html",
          "/manifest.webmanifest.json",
          "/*.css",
          "/*.js"
        ]
      }
    }, {
      "name": "assets",
      "installMode": "lazy",
      "updateMode": "prefetch",
      "resources": {
        "files": [
          "/assets/**"
        ]
      }
    }
  ],
  "navigationUrls" : 
  [
    "/**",  
    "!/**/*.*",  
    "!/**/*__*",   
    "!/**/*__*/**",
    "!/robots.txt",
    "!/api/**",
    "!/connect/**",
    "!/identity/**",
    "!/.well-known/**",
    "!/signin-google",
    "!/signin-microsoft",
    "!/signin-facebook",
    "!/signin-twitter",
    "!/privacy",
    "!/terms-of-service",
    "!/set-culture",
    "!/server-error"
  ]
}
